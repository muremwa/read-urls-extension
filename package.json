{
	"name": "read-urls",
	"displayName": "django-urls-reader",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:read-urls.readUrls"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "read-urls.readUrls",
				"title": "Read Urls"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "django-urls",
					"title": "Your Django project URLs",
					"icon": "media/icon.svg"
				}
			]
		},
		"views": {
			"django-urls": [
				{
					"id": "project-urls",
					"name": "Detected urls",
					"icon": "media/icons.svg",
					"contextualTitle": "Urls Explorer"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "project-urls",
				"contents": "No URLs Detected. \n Read the django documentation on [urls](https://django.org/urls)."
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"walk": "^2.3.14"
	}
}
